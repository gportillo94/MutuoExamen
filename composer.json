{
    "name": "mutuo/prueba-guillermo-portillo",
    "require": {
        "php": ">=5.6",
        "monolog/monolog": "^1.17",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "tuupola/slim-jwt-auth": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
